<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.EculturetoolA" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/color_Primary_500</item>
        <item name="colorPrimaryVariant">@color/color_Primary_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/color_Accent_500</item>
        <item name="colorSecondaryVariant">@color/color_Accent_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/color_Primary_700</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.EculturetoolA.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.EculturetoolA.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.EculturetoolA.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>